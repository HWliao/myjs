{
  "extends": "airbnb",
  "plugins": [
    "compat"
  ],
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true
  },
  "rules": {
    "no-unexpected-multiline": 2,
    "compat/compat": 2,
    "no-underscore-dangle": 0,
    "no-trailing-spaces": ["error", { "skipBlankLines": true }],
    "no-plusplus": ["error", { "allowForLoopAfterthoughts": true }],
    "import/prefer-default-export": "off"
  }
}
