{
  "presets": [
    "react",
    "es2015",
    "env",
    "stage-0",
    "stage-1"
  ],
  "plugins": [
    "transform-class-properties",
    ["transform-runtime", {
      "helpers": false,
      "polyfill": true,
      "regenerator": true
    }],
    "add-module-exports",
    "transform-object-rest-spread"
  ]
}
